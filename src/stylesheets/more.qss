QFrame{
	font-family: &quot;Calibri&quot;;
}
/**********页签项**********/
/*页签项默认状态*/
QTabBar::tab {
		font-family: &quot;Calibri&quot;;
		height: 25px;
        color: rgb(79, 79, 79);
        background: transparent;
		font: 14px;
		margin-right: 10px;
		margin-left: 10px;
}
QTabBar::tab:hover {
		font: 15px;
        border-bottom: 2px solid rgb(155, 155, 155);
        color: black;
		height: 25px;
		margin-right: 10px;
		margin-left: 10px;
}
QTabBar::tab:selected {
		font: 15px solid;
		color: black;
		border-bottom: 2px solid rgb(45, 105, 65);
}
QTabWidget#tabWidget::pane {
		border: 1px solid rgb(220, 220, 220);
        background: rgb(250, 250, 250);
        margin-top: 12px;
		color: black;
		border-radius: 10px;
}

/**********按钮**********/
QPushButton{
        border-radius: 8px;
        border: none;
        height: 30px;
		font-family: &quot;Calibri&quot;;
		font: 12px;
}
QPushButton:enabled {
		background-color: rgb(0, 126, 57);
        color: white;
}
QPushButton:!enabled {
        background-color: rgb(0, 56, 0);
        color: rgb(200, 200, 200);
}
QPushButton:enabled:hover{
        background: rgb(0, 106, 37);
}
QPushButton:enabled:pressed{
        background: rgb(234, 178, 70);
		font: bold;
}

QPushButton[Gray = &quot;true&quot;] {
		border-radius: 8px;
		border: 1px solid rgb(200, 200, 200);
        color: black;
}
QPushButton[Gray = &quot;true&quot;]:enabled {
		background: rgb(235, 235, 235);
        color: black;
}
QPushButton[Gray = &quot;true&quot;]:!enabled {
        background-color: rgb(92, 92, 92);
        color: rgb(200, 200, 200);
}
QPushButton[Gray = &quot;true&quot;]:enabled:hover {
        background: rgb(220, 220, 220);
}
QPushButton[Gray = &quot;true&quot;]:enabled:pressed {
        background: rgb(210, 210, 210);
		font: bold;
}

QPushButton[Orange = &quot;true&quot;] {
		border-radius: 8px;
        color: white;
}
QPushButton[Orange = &quot;true&quot;]:enabled {
		background: rgb(204, 88, 21);
}
QPushButton[Orange = &quot;true&quot;]:!enabled {
        background-color: rgb(78, 34, 9);
        color: rgb(200, 200, 200);
}
QPushButton[Orange = &quot;true&quot;]:enabled:hover {
        background: rgb(165, 72, 18);
}
QPushButton[Orange = &quot;true&quot;]:enabled:pressed {
        background: rgb(234, 178, 70);
		font: bold;
}
QPushButton[Blue = &quot;true&quot;] {
		border-radius: 8px;
        color: white;
}
QPushButton[Blue = &quot;true&quot;]:enabled {
		background: rgb(31, 91, 182);
}
QPushButton[Blue = &quot;true&quot;]:!enabled {
        background-color: rgb(0, 31, 82);
        color: rgb(200, 200, 200);
}
QPushButton[Blue = &quot;true&quot;]:enabled:hover {
        background: rgb(21, 81, 172);
}
QPushButton[Blue = &quot;true&quot;]:enabled:pressed {
        background: rgb(234, 178, 70);
		font: bold;
}

/**********复选框**********/
QCheckBox{
		font-family: &quot;Calibri&quot;;
		font: 12px;
}
QCheckBox:enabled:checked{
        color: rgb(0, 106, 37);
}
