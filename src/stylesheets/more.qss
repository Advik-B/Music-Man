/***********拖动条***********/
QSlider::groove:horizontal{ 
          height: 10px;  
          border:1px solid rgb(220, 220, 220);
          border-radius:5px;    
          background: rgb(220, 220, 220);
		  margin-right: 10px;
		  margin-left: 10px;
} 
QSlider::handle:horizontal{ 
		image:url(./Textures/SliderHandler.png);
		margin: -10px;
		height: 20px;
		width: 20px;
		
} 
QSlider::handle:horizontal:hover{ 
		height: 22px;
		width: 22px;
		
} 
QSlider[Orange = &quot;true&quot;]::handle:horizontal:hover{ 
		height: 22px;
		width: 22px;
		
} 
QSlider[Orange = &quot;true&quot;]::sub-page:horizontal{
		border: 1px solid rgb(220, 220, 220);
 		border-radius:5px;
		font-family: &quot;Calibri&quot;;
